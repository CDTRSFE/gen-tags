{
  "name": "gen-tags",
  "displayName": "Gen Tags",
  "publisher": "zhaoqing",
  "description": "gen tag",
  "version": "1.2.4",
  "engines": {
    "vscode": "^1.82.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CDTRSFE/gen-tags"
  },
  "icon": "src/assets/icon.png",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onView:gen.tags"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "views": {
      "gen-tags": [
        {
          "type": "webview",
          "id": "gen.tags",
          "name": "Gen Tags"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "gen-tags",
          "title": "Gen Tags",
          "icon": "src/assets/icon.svg"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "gentags.refresh",
          "when": "view == gen.tags",
          "group": "navigation"
        }
      ]
    },
    "commands": [
      {
        "command": "gentags.refresh",
        "title": "Refresh",
        "icon": "$(refresh)"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "pnpm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "pnpm run compile-tests && pnpm run compile && pnpm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.3",
    "@types/node": "18.x",
    "@types/vscode": "^1.70.0",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "@vscode/test-electron": "^2.3.6",
    "eslint": "^8.52.0",
    "glob": "^10.3.10",
    "mocha": "^10.2.0",
    "spawn-command": "^0.0.2-1",
    "ts-loader": "^9.5.0",
    "typescript": "^5.2.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "date-fns": "^2.30.0"
  }
}
